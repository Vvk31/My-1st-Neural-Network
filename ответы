import pandas as pd
orders = pd.read_excel('C:/users/1/desktop/orders.xlsx')
orders.head()
orderlines = pd.read_excel('C:/users/1/desktop/orderlines.xlsx')
data = orders.merge(orderlines)
data.head()
ct =data['ProductId'].value_counts()
data1 = data.query("DateTime >='2019-01-01' & DateTime <='2019-01-31'")
gr = data1.groupby('ProductId')['Price'].sum()
gr.head()
top = gr.sort_values(ascending = False)
print(top)
